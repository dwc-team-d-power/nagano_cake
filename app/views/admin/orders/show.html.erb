<div class="container">
  <h1>注文一覧画面</h1>
     <table class="table">
        <thead>
          <tr>
            <th>購入日時</th>
            <th>購入者</th>
            <th>注文個数</th>
            <th>入金ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @admins_orders.each do |p| %>
           <tr>
            <td><%= link_to p.purchase_date.strftime("%Y-%m-%d %H:%M"), admins_order_path(p.id), data: {"turbolinks" => false}  %></td>
            <td><%= .last_name %><%= p.user.first_name %></td>
            <td><%= p.user.email  %></td>
            <td><%= p.order_status_i18n  %></td>
           </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  <%= paginate @admins_orders, class: "paginate" %>
</div>